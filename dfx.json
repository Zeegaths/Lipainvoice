{
  "canisters": {
    "lipa_backend": {
      "main": "src/lipa_backend/main.mo",
      "type": "motoko"
    },
    "ic_siwb_provider": {
      "type": "custom",
      "candid": "packages/ic_siwb_provider/ic_siwb_provider.did",
      "wasm": "target/wasm32-unknown-unknown/release/ic_siwb_provider.wasm",
      "build": [
        "cargo build --target wasm32-unknown-unknown --release -p ic_siwb_provider"
      ]
    },
    "lipa_frontend": {
      "source": ["src/lipa_frontend/dist"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}